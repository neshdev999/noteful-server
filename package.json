{
    "name": "noteful-server",
    "version": "1.0.0",
    "description": "express boilerplate project",
    "main": "index.js",
    "scripts": {
        "test": "mocha --require test/setup.js",
        "dev": "nodemon src/server.js",
        "migrate": "postgrator --config postgrator-config.js",
        "migrate:test": "env NODE_ENV=test npm run migrate",
        "migrate:production": "set env SSL=true & set env DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
        "start": "node src/server.js",
        "predeploy": "npm audit && npm run migrate:production",
        "deploy": "git push heroku main",
        "postdeploy": "npm run migrate:production"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/neshdev999/express-boilerplate.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/neshdev999/express-boilerplate/issues"
    },
    "homepage": "https://github.com/neshdev999/express-boilerplate#readme",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "helmet": "^4.6.0",
        "knex": "^0.95.6",
        "morgan": "^1.10.0",
        "pg": "^8.6.0",
        "postgrator-cli": "^3.2.0",
        "xss": "^1.0.9"
    },
    "devDependencies": {
        "chai": "^4.3.4",
        "eslint": "^7.25.0",
        "eslint-plugin-react": "^7.23.2",
        "mocha": "^8.3.2",
        "nodemon": "^2.0.7",
        "supertest": "^6.1.3"
    }
}